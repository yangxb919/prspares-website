{
  "name": "prspares",
  "version": "0.1.0",
  "private": true,
  "description": "PRSPARES - Excellence in Mobile Repair Parts and OEM Components",
  "scripts": {
    "dev": "next dev",
    "build": "npm run optimize:images && next build && next-sitemap && npm run compress:build",
    "build:analyze": "npm run build && npm run analyze:bundle",
    "build:production": "NODE_ENV=production npm run build && npm run compress:build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:images:force": "rm -rf public/images/optimized && npm run optimize:images",
    "compress:build": "node scripts/compress-build.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:webpack": "ANALYZE=true npm run build",
    "performance:test": "npm run build:production && npm run analyze:bundle"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@n8n/chat": "^0.27.1",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.7",
    "lucide-react": "^0.338.0",
    "next": "14.1.0",
    "next-sitemap": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@next/bundle-analyzer": "^15.5.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.16",
    "compression-webpack-plugin": "^11.1.0",
    "critters": "^0.0.23",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.32",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.0",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5"
  }
}
